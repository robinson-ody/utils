  "author": "Robinson Ody",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/robinson-ody/utils/issues"
  },
  "homepage": "https://github.com/robinson-ody/utils#readme",
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  },
  "files": [
    "lib/**/*"
